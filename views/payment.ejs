        		<div align="center">
        			<br>
        			<h2 style="color:#F7A448"> Payment </h2>
        		</div>
        	</div>

            <div class="row">
                
                <div class="container" align="right">

                	<div class="col-lg-6 col-lg-offset-4" align="left">
                		
                		<br><br>

                        <form id="pay" class=" form-inline">
                        
                        <div class="form-group">
                        <label>Card Number</label>
                            <input type="text" class="form-control" placeholder="" style="width: 300px;" id="cardd" autocomplete='off' pattern="[0-9]{13,16}" required/> 
                        </div>

                        <br><br>

                        <div class="form-group">
                        <label>Card Security Code</label>
                            <input type="text" class="form-control" placeholder="" style="width: 300px;" id="codee" autocomplete='off' pattern="[0-9]{3,4}" required/> 
                        </div>

                        <br><br>

                        <div class="form-group">
                            <label>Expiration</label> <br>
                            <select name="" class="form-control" style="width: 195px;" id="monthh" autocomplete='off'>
                                <option selected> 01 - Jan </option>
                                <option> 02 - Feb </option>
                                <option> 03 - Mar </option>
                                <option> 04 - Apr </option>
                                <option> 05 - May </option>
                                <option> 06 - Jun </option>
                                <option> 07 - Jul </option>
                                <option> 08 - Aug </option>
                                <option> 09 - Sep </option>
                                <option> 10 - Oct </option>
                                <option> 11 - Nov </option>
                                <option> 12 - Dec </option>
                            </select>
                            
                            <select name="" class="form-control" style="width: 90px;" id="yearr" autocomplete='off'>
                                <option selected> 2016 </option>
                                <option> 2017 </option>
                                <option> 2018 </option>
                                <option> 2019 </option>
                                <option> 2020 </option>
                                <option> 2021 </option>
                                <option> 2022 </option>
                                <option> 2023 </option>
                                <option> 2024 </option>
                                <option> 2025 </option>
                                <option> 2026 </option>
                            </select>
                        </div>
                        
                        <br><br>

                        <input type="submit" class="btn col-lg-offset-7" value="Submit">

                        <br><br><br><br>

                    </form>
